{
  "name": "react-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "dev:ts": "tsx server.ts",
    "dev:extension": "vite --config vite.config.extension.ts",
    "build": "vite build",
    "build:extension": "cross-env BUILD_TARGET=extension vite build --config vite.config.extension.ts",
    "preview": "vite preview",
    "clean": "rm -rf dist dist-extension",
    "lint": "tsc --noEmit",
    "start": "NODE_ENV=production node server.js",
    "start:ts": "NODE_ENV=production tsx server.ts"
  },
  "dependencies": {
    "@google/genai": "^1.29.0",
    "@tailwindcss/vite": "^4.1.14",
    "@vitejs/plugin-react": "^5.0.4",
    "better-sqlite3": "^12.4.1",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vite": "^6.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.275",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-static-copy": "^1.0.6"
  }
